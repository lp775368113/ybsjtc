<?xml version="1.0" encoding="UTF-8"?>
 
<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"  
    version="2.0">  
    <description><![CDATA["此标签库实现的目的是为了减少jsp标签的书写量,方便风格的统一;如有违背此意愿的调整,请三思。"]]></description>  
    <display-name>"Widget Tags"</display-name>  
    <tlib-version>1.0</tlib-version>  
    <short-name>m</short-name>  
    <uri>/widget-tags</uri>  
    <tag>
  		<description>
  			<![CDATA[mini-Autocomplete扩展:实现实时后台交互的展示;允许自定义展示显示列]]>
  		</description>  
        <name>list_auto</name>  
        <tag-class>com.wondersgroup.framework.tag.Autocomplete_Extend</tag-class>  
        <body-content>JSP</body-content>  
        <attribute>  
            <description>标签描述；例如(姓名)</description>  
            <name>laber</name>  
            <required>true</required>  
            <rtexprvalue>false</rtexprvalue>  
        </attribute> 
        <attribute>  
            <description>标签唯一标识,赋予原标签的id和name；例如(aac001)</description>  
            <name>property</name>  
            <required>true</required>  
            <rtexprvalue>false</rtexprvalue>  
        </attribute>
        <attribute>  
            <description>后台实现路径：例如(/xx01/getDic.do)</description>  
            <name>url</name>  
            <required>true</required>  
            <rtexprvalue>false</rtexprvalue>  
        </attribute>  
         <attribute>  
            <description>引用原始标签</description>  
            <name>textField</name>  
            <required>true</required>  
            <rtexprvalue>false</rtexprvalue>  
        </attribute>
        <attribute>  
            <description>引用原始标签</description>  
            <name>valueField</name>  
            <required>true</required>  
            <rtexprvalue>false</rtexprvalue>  
        </attribute>
        <attribute>  
            <description>智能匹配标签展示内容：例如({'编码':'aaa100','键值':'aaa102','名称':'aaa103'})</description>  
            <name>show</name>  
            <required>true</required>  
            <rtexprvalue>false</rtexprvalue>  
        </attribute>
        <attribute>  
            <description>引用原始标签</description>  
            <name>value</name>  
            <required>false</required>  
            <rtexprvalue>false</rtexprvalue>  
        </attribute>
        <attribute>  
            <description>引用原始标签</description>  
            <name>required</name>  
            <required>false</required>  
            <rtexprvalue>false</rtexprvalue>  
        </attribute>
        <attribute>  
            <description>引用原始标签</description>  
            <name>style</name>  
            <required>false</required>  
            <rtexprvalue>false</rtexprvalue>  
        </attribute>
  	</tag>  
